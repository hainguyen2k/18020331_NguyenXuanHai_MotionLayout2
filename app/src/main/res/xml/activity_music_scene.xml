<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/view"
               motion:framePosition="0"
               android:alpha="1" />
       </KeyFrameSet>
        <OnSwipe />
        <OnSwipe
            motion:touchAnchorId="@+id/view"
            motion:dragDirection="dragDown" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView6"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView6"
            android:layout_width="wrap_content"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginLeft="140dp"
            android:layout_marginStart="140dp"
            android:id="@+id/imageView11" />
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/imageView5"
            motion:layout_constraintTop_toTopOf="@+id/imageView11"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView11"
            android:layout_marginLeft="180dp"
            motion:layout_constraintEnd_toEndOf="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_marginStart="180dp"
            android:id="@+id/imageView12" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="@+id/imageView5"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView5"
            android:layout_width="wrap_content"
            motion:layout_constraintVertical_bias="0.0"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp"
            android:id="@+id/imageView6" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView5"
            motion:layout_constraintStart_toStartOf="@+id/imageView4"
            motion:layout_constraintTop_toBottomOf="@+id/imageView4"
            android:layout_marginTop="20dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView14"
            motion:layout_constraintEnd_toEndOf="@+id/imageView11"
            motion:layout_constraintStart_toStartOf="@+id/imageView5"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            android:layout_marginBottom="44dp"
            motion:layout_constraintHorizontal_bias="0.451" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="@+id/imageView3"
            android:layout_marginTop="192dp"
            motion:layout_constraintHorizontal_bias="0.492"
            motion:layout_constraintEnd_toEndOf="@+id/imageView3"
            android:layout_width="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3"
            android:id="@+id/imageView4" />
        <Constraint
            android:id="@+id/imageView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="62dp"
            motion:layout_editor_absoluteX="58dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="15dp"
            motion:layout_editor_absoluteX="35dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="232dp"
            motion:layout_editor_absoluteX="115dp"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="123dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:id="@+id/view"
            motion:layout_editor_absoluteY="608dp" />
        <Constraint
            android:layout_height="72dp"
            android:layout_width="55dp"
            android:id="@+id/imageView3"
            motion:layout_constraintBottom_toBottomOf="@+id/view"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="33dp"
            android:layout_width="32dp"
            android:id="@+id/imageView5"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="56dp"
            android:layout_marginStart="56dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView3" />
        <Constraint
            android:layout_height="33dp"
            android:layout_width="33dp"
            android:id="@+id/imageView6"
            motion:layout_constraintStart_toEndOf="@+id/imageView5"
            motion:layout_constraintTop_toTopOf="@+id/imageView5"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp" />
        <Constraint
            android:layout_height="33dp"
            android:layout_width="37dp"
            android:id="@+id/imageView11"
            motion:layout_constraintStart_toEndOf="@+id/imageView6"
            motion:layout_constraintTop_toTopOf="@+id/imageView6"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp" />
        <Constraint
            android:layout_height="33dp"
            android:layout_width="37dp"
            android:id="@+id/imageView12"
            motion:layout_constraintTop_toTopOf="@+id/imageView11"
            motion:layout_constraintStart_toEndOf="@+id/imageView11"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" />
        <Constraint
            android:layout_height="32dp"
            android:layout_width="221dp"
            android:id="@+id/imageView4"
            motion:layout_constraintTop_toBottomOf="@+id/imageView6"
            android:layout_marginTop="4dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView4" />
        <Constraint
            motion:layout_editor_absoluteY="470dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="95dp"
            android:layout_width="wrap_content"
            android:id="@+id/imageView14"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView15"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/imageView"
            motion:layout_constraintHorizontal_bias="0.305"
            android:layout_marginTop="52dp" />
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="invisible" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView17"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView18"
            motion:layout_constraintStart_toStartOf="@+id/imageView15"
            motion:layout_constraintEnd_toEndOf="@+id/imageView15"
            motion:layout_constraintTop_toBottomOf="@+id/imageView15"
            android:layout_marginTop="12dp"
            motion:layout_constraintHorizontal_bias="0.507"
            motion:layout_constraintBottom_toTopOf="@+id/view"
            motion:layout_constraintVertical_bias="0.33" />
    </ConstraintSet>
</MotionScene>